                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
	act_immo.Level2 AS Code,
	label2.Label

FROM
	[FisoDWH].[dbo].[Annual_Accounts_Hier] act_immo

INNER JOIN [dbo].[Annual_Accounts_Labels] label2
	ON label2.Level = 2
	AND label2.Type = 'PCN'
	AND label2.Language = 'FR'
	AND label2.Code = act_immo.Level2

INNER JOIN [FisoDWH].[dbo].[account_balance] bal
ON act_immo.PCN = substring(bal.Account,1,len(act_immo.PCN))

WHERE
	act_immo.Type = 'PCN'
	AND act_immo.Level2 IN ('21','22')
	AND bal.customer_code = 'ANT001'

GROUP BY
	act_immo.Level2,
	label2.Label</CommandText>
